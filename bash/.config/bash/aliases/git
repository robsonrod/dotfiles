# git alias
alias g='git'
alias nb='git checkout -b "$USER/$(date +%Y%m%d)"'
alias clone='git clone'
alias gaa='git add . --all'
alias gau='git add -u'
alias gst='git status'
alias gcm='git commit -v -m'
alias gc='git checkout'
alias gch='git checkout -b'
alias gdf='git diff'
alias gpl='git pull'
alias gll="git log --graph --pretty=format:'%C(bold)%h%Creset%C(magenta)%d%Creset %s %C(yellow)<%an> %C(cyan)(%cr)%Creset' --abbrev-commit --date=relative"
alias groot='git rev-parse --show-toplevel'
alias fecth='git fetch --all --prune --verbose'
alias reset='git reset HEAD'
alias gpp='git pull && git push'
alias gps='git push origin'
alias gclear='git clean -xfd'
alias dbranch='git branch -Dd'
alias gmain='git checkout main'
alias gsl='git stash list'
alias gsp='git stash pop'
