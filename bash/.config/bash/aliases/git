# git alias
alias g='git'
alias nb='git checkout -b "$USER/$(date +%Y%M%d)"'
alias gc='git clone'
alias ga='git add . --all'
alias gac='git add -u'
alias gs='git status'
alias gci='git commit -v -m'
alias gco='git checkout'
alias gcb='git checkout -b'
alias gd='git diff'
alias gu='git pull'
alias gl="git log --graph --pretty=format:'%C(bold)%h%Creset%C(magenta)%d%Creset %s %C(yellow)<%an> %C(cyan)(%cr)%Creset' --abbrev-commit --date=relative"
alias gr='git rev-parse --show-toplevel'
alias gf='git fetch --all --prune --verbose'
alias grt='git reset HEAD'
alias gpp='git pull && git push'
alias gp='git push origin'
alias gclear='git clean -xfd'
alias gbd='git branch -D'
alias gcm='git checkout master'
alias gsl="git stash list"
alias gsp="git stash pop"
